<project xmlns="http://maven.apache.org/POM/4.0.0"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/xsd/maven-4.0.0.xsd">
	<modelVersion>4.0.0</modelVersion>

	<parent>
		<groupId>se.uu.ub.cora</groupId>
		<artifactId>cora-parent</artifactId>
		<version>1.0.2</version>
	</parent>

	<artifactId>alvinmixedstorage</artifactId>
	<version>3.1-SNAPSHOT</version>
	<packaging>jar</packaging>

	<name>Alvin Mixed Storage</name>

	<properties>
		<logger.version>2.0.0</logger.version>
		<coradata.version>3.1.0</coradata.version>
		<gatekeeper.version>3.1.0</gatekeeper.version>
		<storage.version>3.1.0</storage.version>
		<searchstorage.version>3.1.0</searchstorage.version>
		<httphandler.version>2.0.0</httphandler.version>
		<basicstorage.version>3.1.0</basicstorage.version>
		<sqldatabase.version>2.2.0</sqldatabase.version>
		<messaging.version>2.1.0</messaging.version>
		<rabbitmq.version>2.0.0</rabbitmq.version>
		<converter.version>1.0-SNAPSHOT</converter.version>
		<xmlconverter.version>1.0-SNAPSHOT</xmlconverter.version>
	</properties>

	<scm>
		<connection>scm:git:https://github.com/lsu-ub-uu/alvin-mixedstorage.git</connection>
		<developerConnection>scm:git:https://github.com/lsu-ub-uu/alvin-mixedstorage.git</developerConnection>
		<url>https://github.com/lsu-ub-uu/alvin-mixedstorage.git</url>
		<tag>HEAD</tag>
	</scm>

